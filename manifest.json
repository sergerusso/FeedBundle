{
  "name": "FeedBundle",
  "version": "0.90",
  "description": "",
  "author":"Serge Russo <contact@sergerusso.com>",
  "manifest_version": 2,
  "permissions": [],
  "optional_permissions": [ "webRequest", "webRequestBlocking", "<all_urls>" ],
  "browser_action": {
    "default_icon": "assets/images/favicon.png",
    "default_title": "FeedBundle"
  },
  "icons": {
    "48": "assets/images/favicon.png",
    "128": "assets/images/favicon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "assets/js/lib/pouchdb-6.0.7.min.js",
      "assets/js/chrome.js"
    ],
    "persistent": false
  }
}
