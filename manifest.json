{
  "name": "FeedBundle - Online RSS News Reader",
  "version": "0.95",
  "description": "Make your own collection of feeds, organize and browse it with FeedBundle.",
  "author":"Serge Russo <contact@sergerusso.com>",
  "manifest_version": 2,
  "permissions": ["alarms", "storage", "unlimitedStorage"],
  "optional_permissions": [ "webRequest", "webRequestBlocking", "<all_urls>" ],
  "browser_action": {
    "default_icon": "assets/images/favicon.png",
    "default_title": "FeedBundle"
  },
  "icons": {
    "48": "assets/images/favicon.png",
    "128": "assets/images/favicon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "page":"background.html",
    "persistent": false
  }
}
